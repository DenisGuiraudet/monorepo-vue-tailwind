{
  "name": "@guidap/ui",
  "version": "0.0.0",
  "private": true,
  "main": "src/index.ts",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "scripts": {
    "test:unit": "vitest",
    "type-check": "vue-tsc --noEmit -p ../config/tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@vue/test-utils": "^2.3.2",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "vue-tsc": "^1.6.5"
  }
}
